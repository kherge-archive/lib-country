{
    "name": "phine/country",
    "description": "A PHP library for country and subdivision data.",
    "keywords": ["countries","country"],
    "homepage": "https://github.com/phine/lib-country",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/phine/lib-country/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "phine/exception": "~1.0",
        "phine/test": "~1.0"
    },
    "require-dev": {
        "league/phpunit-coverage-listener": "~1.0"
    },
    "autoload": {
        "psr-0": {
            "Phine\\Country": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
